{
  "name": "backend",
  "version": "1.0.0",
  "description": "Notes's server",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "build:ui": "rm -rf build && cd ../lista_notas && npm run build --prod && cp -r build ../server_nodejs_notes/",
    "deploy": "git push heroku master",
    "deploy:full": "npm run build:ui && git add . && git commit -m 'hola' && npm run deploy",
    "logs:prod": "heroku logs --tail"
  },
  "author": "Giancarlo Guerra",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongoose": "^5.12.12",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
